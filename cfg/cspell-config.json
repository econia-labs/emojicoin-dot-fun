{
  "dictionaries": [
    "project-words"
  ],
  "dictionaryDefinitions": [
    {
      "addWords": true,
      "name": "project-words",
      "path": "./cspell-dictionary.txt"
    },
    {
      "addWords": true,
      "name": "frontend-words",
      "path": "./cspell-frontend-dictionary.txt"
    }
  ],
  "ignorePaths": [
    "/project-words.txt",
    "**/package.json",
    "**/pnpm-lock.yaml"
  ],
  "overrides": [
    {
      "dictionaries": [
        "frontend-words"
      ],
      "filename": "**/src/typescript/frontend/**/*.{css,html,ts,tsx,js,jsx,json}"
    }
  ]
}
