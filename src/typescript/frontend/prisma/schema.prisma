generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model CoinsList {
  id        String   @id @default(auto()) @map("_id") @db.ObjectId
  data      Json?
  titleSlug String   @unique
  emojiSlug String   @unique
  emojis    String[]
  createdAt DateTime @default(now())
  meta      Meta
}

type Meta {
  title       String
  description String
  imageURL    String
}
