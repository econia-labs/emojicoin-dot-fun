{
  "dependencies": {
    "@econia-labs/emojicoin-sdk": "0.5.0-rc.arena",
    "axios": ">=0.28.0",
    "big.js": "^6.2.2",
    "find-git-root": "^1.0.4"
  },
  "description": "Emojicoin scripts",
  "devDependencies": {
    "@types/big.js": "^6.2.2",
    "@types/jest": "^29.5.14",
    "@types/lodash": "^4.17.15",
    "@types/node": "^20.16.11",
    "@typescript-eslint/eslint-plugin": "^7.18.0",
    "@typescript-eslint/parser": "^7.18.0",
    "axios": ">=0.28.0",
    "dotenv-cli": "^7.4.2",
    "eslint": "^8.57.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^18.0.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-unused-imports": "^3.2.0",
    "graphql-request": "^7.1.2",
    "jest": "^29.7.0",
    "kolorist": "^1.8.0",
    "lodash": "^4.17.21",
    "prettier": "^3.3.3",
    "tree-kill": "^1.2.2",
    "tsx": "^4.19.2",
    "typescript": "5.6.2"
  },
  "engines": {
    "node": ">=v20.12.2"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "name": "@econia-labs/emojicoin-scripts",
  "packageManager": "pnpm@9.4.0+sha512.f549b8a52c9d2b8536762f99c0722205efc5af913e77835dbccc3b0b0b2ca9e7dc8022b78062c17291c48e88749c70ce88eb5a74f1fa8c4bf5e18bb46c8bd83a",
  "peerDependencies": {
    "@aptos-labs/ts-sdk": "1.27.1"
  },
  "scripts": {
    "_format": "prettier 'src/**/*.ts' 'tests/**/*.ts' '.eslintrc.js'",
    "build": "tsc",
    "build:no-checks": "tsc --skipLibCheck",
    "check": "tsc -p tests/tsconfig.json --noEmit",
    "clean": "rm -rf .turbo && rm -rf node_modules && rm -rf dist",
    "format": "pnpm _format --list-different --write",
    "format:check": "pnpm _format --check",
    "lint": "eslint --max-warnings=0 'src/**/*.ts' 'tests/**/*.ts' -c .eslintrc.js",
    "lint:fix": "pnpm lint --fix",
    "load-env": "dotenv -e .env -v NODE_OPTIONS=\"--conditions=react-server\"",
    "load-env-local": "dotenv -e ../ci.env -e ../example.local.env -v NODE_OPTIONS=\"--conditions=react-server\"",
    "load-env-mainnet": "dotenv -e ../example.mainnet.env -v NODE_OPTIONS=\"--conditions=react-server\"",
    "load-env-testnet": "dotenv -e ../example.testnet.env -v NODE_OPTIONS=\"--conditions=react-server\"",
    "arena:bots": "pnpm load-env-local -- tsx src/arena/local/bots.ts",
    "arena:init": "pnpm load-env-local -- tsx src/arena/local/init.ts"
  },
  "private": true,
  "version": "0.0.1"
}
