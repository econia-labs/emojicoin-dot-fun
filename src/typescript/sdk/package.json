{
  "dependencies": {
    "@aptos-labs/ts-sdk": "^1.16.0",
    "@keyvhq/core": "^2.1.1",
    "@noble/hashes": "^1.4.0",
    "@supabase/postgrest-js": "^1.15.2",
    "dotenv": "^16.4.5",
    "find-git-root": "^1.0.4",
    "postgres": "^3.4.4"
  },
  "description": "TypeScript SDK for Econia's Emojicoin Dot Fun",
  "devDependencies": {
    "@aptos-labs/aptos-cli": "^0.1.2",
    "@types/jest": "^29.5.12",
    "@types/node": "^20.12.7",
    "@typescript-eslint/eslint-plugin": "^6.14.0",
    "@typescript-eslint/parser": "^6.14.0",
    "eslint": "^8.55.0",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-import": "^2.29.0",
    "eslint-plugin-unused-imports": "^3.1.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "tree-kill": "^1.2.2",
    "ts-jest": "^29.1.1",
    "ts-node": "^10.9.2",
    "typescript": "5.3.3"
  },
  "engines": {
    "node": ">=v20.12.2"
  },
  "exports": {
    ".": "./src/index.ts"
  },
  "files": [
    "dist"
  ],
  "license": "Apache-2.0",
  "main": "dist/src/index.js",
  "name": "@econia-labs/emojicoin-sdk",
  "packageManager": "pnpm@9.1.1+sha512.14e915759c11f77eac07faba4d019c193ec8637229e62ec99eefb7cf3c3b75c64447882b7c485142451ee3a6b408059cdfb7b7fa0341b975f12d0f7629c71195",
  "scripts": {
    "_format": "prettier 'src/**/*.ts' 'tests/**/*.ts' '.eslintrc.js'",
    "build": "pnpm clean && tsc",
    "check": "tsc --noEmit",
    "clean": "rm -rf dist",
    "e2e-test": "pnpm jest tests/e2e",
    "format": "pnpm _format --write",
    "format:check": "pnpm _format --check",
    "generate-data": "pnpm tsx src/utils/mock-data.ts",
    "lint": "eslint 'src/**/*.ts' 'tests/**/*.ts' -c .eslintrc.js",
    "lint:fix": "pnpm lint --fix",
    "mock-test": "pnpm jest tests/mock",
    "pre-commit": "pnpm run pre-commit:install && pnpm run pre-commit:run",
    "pre-commit:install": "pre-commit install -c ../../../cfg/pre-commit-config.yaml",
    "pre-commit:run": "pre-commit run --all-files -c ../../../cfg/pre-commit-config.yaml",
    "test": "pnpm jest",
    "unit-test": "NO_TEST_SETUP=true pnpm jest tests/unit"
  },
  "typings": "dist/src/index.d.ts",
  "version": "0.0.1"
}
