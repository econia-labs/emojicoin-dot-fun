{
  "compilerOptions": {
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "declarationDir": "types",
    "declarationMap": true,
    "esModuleInterop": true,
    "experimentalDecorators": true,
    "forceConsistentCasingInFileNames": true,
    "isolatedModules": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "module": "esnext",
    "moduleDetection": "force",
    "moduleResolution": "bundler",
    "noEmit": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "paths": {
      "@/move-modules": [
        "./src/emojicoin_dot_fun/move-modules"
      ],
      "@/move-modules/*": [
        "./src/emojicoin_dot_fun/move-modules/*"
      ]
    },
    "preserveWatchOutput": true,
    "resolveJsonModule": true,
    "skipLibCheck": true,
    "sourceMap": true,
    "strict": true,
    "target": "esnext",
    "types": [
      "node",
      "jest"
    ],
    "verbatimModuleSyntax": true
  },
  "exclude": [
    "node_modules"
  ],
  "include": [
    "src",
    "tsup.config.ts"
  ]
}
