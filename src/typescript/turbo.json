{
  "$schema": "https://turbo.build/schema.json",
  "envMode": "loose",
  "globalDependencies": [
    "**/*.env.*",
    "**/*.env",
    "**/tsconfig.json",
    "**/package.json"
  ],
  "globalEnv": [],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "build:debug": {
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "build:no-checks": {
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "build:test": {
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "check": {
      "cache": false,
      "outputs": []
    },
    "check:ci": {
      "cache": false,
      "outputs": []
    },
    "check:tests": {
      "cache": false,
      "outputs": []
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "playwright:install": {
      "outputs": []
    },
    "start": {
      "outputs": [],
      "persistent": true
    },
    "submodule": {
      "outputs": []
    },
    "test:e2e": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:arena-candlesticks": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:arena-general": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:arena-restart": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:arena-ws": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:base-general": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:base-ws": {
      "cache": false,
      "outputs": []
    },
    "test:e2e:candlesticks": {
      "cache": false,
      "outputs": []
    },
    "test:unit": {
      "cache": false,
      "outputs": []
    }
  }
}
