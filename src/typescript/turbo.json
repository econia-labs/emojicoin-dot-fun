{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "INBOX_URL",
    "SHORT_REVALIDATE"
  ],
  "tasks": {
    "build": {
      "dependsOn": [
        "^build"
      ],
      "outputs": [
        "dist/**",
        ".next/**"
      ]
    },
    "clean": {
      "outputs": []
    },
    "dev": {
      "cache": false
    },
    "format": {
      "outputs": []
    },
    "format:check": {
      "outputs": []
    },
    "lint": {
      "outputs": []
    },
    "lint:fix": {
      "outputs": []
    },
    "start": {
      "dependsOn": [
        "build"
      ],
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
